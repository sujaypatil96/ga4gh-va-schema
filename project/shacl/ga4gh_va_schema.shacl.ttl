    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Data Set" ;
    sh:property [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 15 ;
            sh:path vacoreim:type ],
        [ sh:class vacoreim:Document ;
            sh:description "A document in which the information content carried by the Information Entity is expressed" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path vacoreim:reportedIn ],
        [ sh:description "The logical identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system. The identified entity may have a different 'id' in a different system." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path vacoreim:id ],
        [ sh:description "The version of the Data Set (use this field used in cases where version is not reflected in an identifier associated with the Data Set)" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path vacoreim:version ],
        [ sh:description "The date the information content expressed in this entity was generated." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path vacoreim:dateAuthored ],
        [ sh:description "The URL/web address of a digital resource representing the entity, or providing information about it." ;
            sh:order 12 ;
            sh:path vacoreim:urls ],
        [ sh:description "The type of license that dictates legal permissions for how the Data Set can be used - typically referenced by its URL." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path vacoreim:license ],
        [ sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path vacoreim:label ],
        [ sh:description "A business identifier or accession number for the entity, typically as provided by an external system or authority, that is globally unique and persists across implementing systems." ;
            sh:order 10 ;
            sh:path vacoreim:identifiers ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 17 ;
            sh:path vacoreim:extensions ],
        [ sh:class vacoreim:Method ;
            sh:description "A particular plan specification that describes all or part of the process that led to creation of the reported information (e.g. a specific experimental protocol, analysis specification, cohort selection criteria, interpretation guideline, etc.)." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path vacoreim:specifiedBy ],
        [ sh:class vacoreim:Contribution ;
            sh:description "A specific contribution made by some Agent to the creation, modification, or validation of the information represented in the Information Entity." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path vacoreim:contributions ],
        [ sh:class vacoreim:RecordMetadata ;
            sh:description "A reusable structure that encapsulates provenance metadata about the present record/data object  (as opposed to provenance information about the real world entity this record/data object represents)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path vacoreim:recordMetadata ],
        [ sh:class vacoreim:InformationEntity ;
            sh:description "An information resource from which the Information Entity is derived, in whole or in part" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path vacoreim:derivedFrom ],
        [ sh:description "The date when a version-level Data Set was released." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path vacoreim:releaseDate ],
        [ sh:class vacoreim:Coding ;
            sh:description "A specific type of data set the DataSet object represents." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path vacoreim:subtype ],
        [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 16 ;
            sh:path vacoreim:description ],
        [ sh:description "Cross-references to database identifier(s) representing the same (or a closely related) entity or concept as the Entity." ;
            sh:order 13 ;
            sh:path vacoreim:xrefs ] ;
