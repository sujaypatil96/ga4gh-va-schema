            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path vacoreim:derivedFrom ],
        [ sh:description "Cross-references to database identifier(s) representing the same (or a closely related) entity or concept as the Entity." ;
            sh:order 14 ;
            sh:path vacoreim:xrefs ],
        [ sh:class vacoreim:Coding ;
            sh:description "The direction of support that the Evidence Line is determined to provide for its target Proposition (can be supporting, disputing, or neutral)" ;
