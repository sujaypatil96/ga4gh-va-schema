            sh:minCount 1 ;
            sh:order 16 ;
            sh:path vacoreim:type ],
        [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 17 ;
            sh:path vacoreim:description ],
        [ sh:class vacoreim:Extension ;
