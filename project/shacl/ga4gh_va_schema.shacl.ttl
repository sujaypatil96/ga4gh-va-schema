@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix vacoreim: <https://example.org/vacoreim/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

vacoreim:Activity a sh:NodeShape ;
    sh:closed true ;
    sh:description "An action or set of actions performed by an agent, that occurs over a period of time. Activities may use, generate, modify, move, or destroy one or more entities." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Activity" ;
    sh:property [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 12 ;
            sh:path vacoreim:type ],
        [ sh:description "Cross-references to database identifier(s) representing the same (or a closely related) entity or concept as the Entity." ;
            sh:order 10 ;
            sh:path vacoreim:xrefs ],
        [ sh:class vacoreim:Agent ;
            sh:description "An Agent who participated in executing the Activity." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path vacoreim:performedBy ],
        [ sh:description "A business identifier or accession number for the entity, typically as provided by an external system or authority, that is globally unique and persists across implementing systems." ;
            sh:order 7 ;
            sh:path vacoreim:identifiers ],
        [ sh:description "The date (or a specifiec time on a date) that the Activitywas performed" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path vacoreim:date ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path vacoreim:extensions ],
        [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 13 ;
            sh:path vacoreim:description ],
        [ sh:class vacoreim:Entity ;
            sh:description "An entity that was input into the Activity (could be material or information)" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path vacoreim:input ],
        [ sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path vacoreim:label ],
        [ sh:class vacoreim:RecordMetadata ;
            sh:description "A reusable structure that encapsulates provenance metadata about the present record/data object  (as opposed to provenance information about the real world entity this record/data object represents)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path vacoreim:recordMetadata ],
        [ sh:description "The URL/web address of a digital resource representing the entity, or providing information about it." ;
            sh:order 9 ;
            sh:path vacoreim:urls ],
        [ sh:class vacoreim:Method ;
            sh:description "A directive specification followed in performing an Activity, that describes how it should be executed." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path vacoreim:specifiedBy ],
        [ sh:class vacoreim:Coding ;
            sh:description "A specific type of activity this Activity object may represent" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path vacoreim:subtype ],
        [ sh:class vacoreim:Entity ;
            sh:description "An entity that was output from the Activity (could be material or information)" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path vacoreim:output ],
        [ sh:description "The logical identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system. The identified entity may have a different 'id' in a different system." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path vacoreim:id ] ;
    sh:targetClass vacoreim:Activity .

vacoreim:Expression a sh:NodeShape ;
    sh:closed true ;
    sh:description "A structure for labels representing systematic expressions that describe an entity, as generated by formal nomenclatures (e.g. HGVS for genetic variants, ISCN for karyotypes, HLA nomenclature for HLA genes/alleles)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Expression" ;
    sh:property [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path vacoreim:description ],
        [ sh:description "The version of the nomenclature system." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path vacoreim:systemVersion ],
        [ sh:description "The nomenclature system that created and assigned the expression." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path vacoreim:system ],
        [ sh:description "A free-text rendering of the expression used as a label for the entity" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path vacoreim:value ],
        [ sh:description "A URL for the nomenclature system" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path vacoreim:systemURL ],
        [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path vacoreim:type ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path vacoreim:extensions ] ;
    sh:targetClass vacoreim:Expression .

vacoreim:Statement a sh:NodeShape ;
    sh:closed true ;
    sh:description "An information entity expressing a declarative sentence that is either true or false." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Statement" ;
    sh:property [ sh:class vacoreim:Coding ;
            sh:description "A term describing the asserting agent's level of confidence about the Statement's proposition (i.e. how confident they are that it is true or false)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path vacoreim:confidenceLevel ],
        [ sh:class vacoreim:InformationEntity ;
            sh:description "An information resource from which the Information Entity is derived, in whole or in part" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path vacoreim:derivedFrom ],
        [ sh:description "A business identifier or accession number for the entity, typically as provided by an external system or authority, that is globally unique and persists across implementing systems." ;
            sh:order 18 ;
            sh:path vacoreim:identifiers ],
        [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 23 ;
            sh:path vacoreim:type ],
        [ sh:class vacoreim:Coding ;
            sh:description "A term indicating the overall strength of support for or against the Statement's proposition, as provided by all evidence evaluated." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path vacoreim:evidenceLevel ],
        [ sh:description "The URL/web address of a digital resource representing the entity, or providing information about it." ;
            sh:order 20 ;
            sh:path vacoreim:urls ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 25 ;
            sh:path vacoreim:extensions ],
        [ sh:class vacoreim:RecordMetadata ;
            sh:description "A reusable structure that encapsulates provenance metadata about the present record/data object  (as opposed to provenance information about the real world entity this record/data object represents)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 22 ;
            sh:path vacoreim:recordMetadata ],
        [ sh:class vacoreim:EvidenceLine ;
            sh:description "A discrete, independent argument relevant to the validity of the Proposition put forth in the Statement, which is based on the interpretation of one or more pieces of information as evidence." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path vacoreim:hasEvidenceLines ],
        [ sh:description "Cross-references to database identifier(s) representing the same (or a closely related) entity or concept as the Entity." ;
            sh:order 21 ;
            sh:path vacoreim:xrefs ],
        [ sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:order 19 ;
            sh:path vacoreim:label ],
        [ sh:class vacoreim:Coding ;
            sh:description "A term indicating whether the confidenceLevel and/or confidenceScore values reflect confidence that the statement's proposition is true or false." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path vacoreim:confidenceDirection ],
        [ sh:class vacoreim:Coding ;
            sh:description "A term that describes a final conclusion about the subject of a Statement's proposition, based on what the proposition claims, and confidence and evidence level and direction assessments." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path vacoreim:conclusion ],
        [ sh:class vacoreim:Method ;
            sh:description "A particular plan specification that describes all or part of the process that led to creation of the reported information (e.g. a specific experimental protocol, analysis specification, cohort selection criteria, interpretation guideline, etc.)." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path vacoreim:specifiedBy ],
        [ sh:class vacoreim:Document ;
            sh:description "A document in which the information content carried by the Information Entity is expressed" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path vacoreim:reportedIn ],
        [ sh:class vacoreim:Coding ;
            sh:description "A term describing a type of evidence used to assess the validity of Statement's proposition (e.g. 'sequence similarity evidence', 'in vitro assay evidence')." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path vacoreim:hasEvidenceOfTypes ],
        [ sh:class vacoreim:InformationEntity ;
            sh:description "A piece of information that represents or contributes to an argument for or against the validity of the Proposition put forth in a Statement." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path vacoreim:hasEvidence ],
        [ sh:description "The date the information content expressed in this entity was generated." ;
            sh:maxCount 1 ;
            sh:order 13 ;
            sh:path vacoreim:dateAuthored ],
        [ sh:class vacoreim:Coding ;
            sh:description "A specific type of statement the Statement object represents (e.g. a 'Variant Pathogenicity Statement', or 'Therapeutic Response Evidence Level Statement')" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path vacoreim:subtype ],
        [ sh:description "The logical identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system. The identified entity may have a different 'id' in a different system." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 17 ;
            sh:path vacoreim:id ],
        [ sh:class vacoreim:DataItem ;
            sh:description "A quantitative score reflecting the overall strength of support for or against the Statement's proposition, as provided by all evidence evaluated." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path vacoreim:evidenceScore ],
        [ sh:description "A term indicating the net direction of all evidence assessed against the Statement's proposition." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path vacoreim:evidenceDirection ],
        [ sh:description "A natural-language expression of what the Statement asserts to be true." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path vacoreim:statementText ],
        [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 24 ;
            sh:path vacoreim:description ],
        [ sh:class vacoreim:DataItem ;
            sh:description "A quantitative score reflecting the asserting agent's level of confidence in the Statement's proposition. For PropositionAssessmentStatements, the direction this confidence pertains to (i.e. the truth or falsehood of the proposition) is captured in the confidenceDirection attribute." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path vacoreim:confidenceScore ],
        [ sh:class vacoreim:Contribution ;
            sh:description "A specific contribution made by some Agent to the creation, modification, or validation of the information represented in the Information Entity." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 12 ;
            sh:path vacoreim:contributions ] ;
    sh:targetClass vacoreim:Statement .

vacoreim:StudyResult a sh:NodeShape ;
    sh:closed true ;
    sh:description "A collection of data items from a single study that are about a particular subject or experimental unit in the study  (along with optional provenance metadata describing how these data items were generated)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Study Result" ;
    sh:property [ sh:description "The URL/web address of a digital resource representing the entity, or providing information about it." ;
            sh:order 11 ;
            sh:path vacoreim:urls ],
        [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 14 ;
            sh:path vacoreim:type ],
        [ sh:description "The date the information content expressed in this entity was generated." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path vacoreim:dateAuthored ],
        [ sh:class vacoreim:DataItem ;
            sh:description "A piece of data from the source Study that is included in the Result, and pertains to the entity that is the 'focus'. Can be data directly about the focus, or metadata relevant to how this data was generated." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path vacoreim:dataItems ],
        [ sh:description "The logical identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system. The identified entity may have a different 'id' in a different system." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 8 ;
            sh:path vacoreim:id ],
        [ sh:class vacoreim:RecordMetadata ;
            sh:description "A reusable structure that encapsulates provenance metadata about the present record/data object  (as opposed to provenance information about the real world entity this record/data object represents)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path vacoreim:recordMetadata ],
        [ sh:class vacoreim:Contribution ;
            sh:description "A specific contribution made by some Agent to the creation, modification, or validation of the information represented in the Information Entity." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path vacoreim:contributions ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path vacoreim:extensions ],
        [ sh:class vacoreim:Method ;
            sh:description "A particular plan specification that describes all or part of the process that led to creation of the reported information (e.g. a specific experimental protocol, analysis specification, cohort selection criteria, interpretation guideline, etc.)." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path vacoreim:specifiedBy ],
        [ sh:description "A business identifier or accession number for the entity, typically as provided by an external system or authority, that is globally unique and persists across implementing systems." ;
            sh:order 9 ;
            sh:path vacoreim:identifiers ],
        [ sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path vacoreim:label ],
        [ sh:description "Cross-references to database identifier(s) representing the same (or a closely related) entity or concept as the Entity." ;
            sh:order 12 ;
            sh:path vacoreim:xrefs ],
        [ sh:class vacoreim:Document ;
            sh:description "A document in which the information content carried by the Information Entity is expressed" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path vacoreim:reportedIn ],
        [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 15 ;
            sh:path vacoreim:description ],
        [ sh:class vacoreim:Entity ;
            sh:description "The specific subject or experimental unit in the Research Study that data in the Result is about, and which guides the selection of data that is included within a single Study Result object." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path vacoreim:focus ],
        [ sh:class vacoreim:DataSet ;
            sh:description "A larger Data Set from which the content of the Result was derived." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path vacoreim:derivedFrom ],
        [ sh:class vacoreim:Coding ;
            sh:description "A specific type of result the StudyResult object represents (e.g. an 'Population Allele Frequency Study Result')" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path vacoreim:subtype ] ;
    sh:targetClass vacoreim:StudyResult .

vacoreim:Utility a sh:NodeShape ;
    sh:closed true ;
    sh:description "An abstract organizational class that groups complex datatype-like classes - which provide re-usable collections of fields that can be plugged into other objects to capture related information." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A free text description of the Element." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path vacoreim:description ],
        [ sh:description "The schema class that is instantiated by the data object.  Must be the name of a class from the VA schema." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path vacoreim:type ],
        [ sh:class vacoreim:Extension ;
            sh:description "A key-value data structure that allows definition of custom fields to capture information not directly supported by the VA specification." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path vacoreim:extensions ] ;
    sh:targetClass vacoreim:Utility .

